{
	"name": "mas.js",
	"version": "1.0.0",
	"description": "Mokiros's API Server - my own API server with 0 dependencies",
	"author": "Mokiros",
	"license": "MIT",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"lint": "eslint src",
		"lint-fix": "eslint --fix src",
		"compile": "tsc",
		"build": "npm run lint && npm run compile"
	},
	"files": [
		"dist/**/*"
	],
	"devDependencies": {
		"@types/node": "^14.14.19",
		"@typescript-eslint/eslint-plugin": "^4.16.1",
		"@typescript-eslint/parser": "^4.16.1",
		"eslint": "^7.21.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "2.4.1",
		"ts-node-dev": "^1.1.6",
		"typescript": "^4.1.3"
	},
	"dependencies": {},
	"prettier": {
		"semi": false,
		"trailingComma": "all",
		"singleQuote": true,
		"printWidth": 120,
		"tabWidth": 4,
		"useTabs": true,
		"bracketSpacing": true,
		"bracketSameLine": false,
		"endOfLine": "lf",
		"arrowParens": "always"
	}
}
